@model Dominio.Entidades.Cliente

    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Cliente @Model.Nome</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label>Código: </label>
                    <label>@Model.Id</label>
                </div>
                <div class="form-group">
                    <label>CPF: </label>
                    <label>@Model.Cpf</label>
                </div>
                <div class="form-group">
                    <label class="">Data de Nascimento: </label>
                    <label>@Model.DataNascimento.ToLocalTime().ToShortDateString()</label>
                </div>
                <div class="form-group">
                    <label>Telefone:  </label>
                    <label>@Model.Telefone</label>
                </div>
                <div class="form-group">
                    <label>Email: </label>
                    <label>@Model.Email</label>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Fechar</button>
             </div>
        </div>
    </div>

